<Application x:Class="QuickJump.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:QuickJump"
             xmlns:helpers="clr-namespace:QuickJump.Helpers"
             >
             <!--ShutdownMode="OnExplicitShutdown"-->
    <Application.Resources>
        <helpers:BoolToVisibilityConverter TrueValue="Visible" FalseValue="Collapsed" x:Key="BoolToVisibilityConverter" />
        <helpers:BoolToVisibilityConverter TrueValue="Collapsed" FalseValue="Visible" x:Key="InvertedBoolToVisibilityConverter" />
        <helpers:CategoryToIconConverter x:Key="CategoryToIconConverter" />
        <helpers:EmptyToVisibilityConverter x:Key="EmptyToVisibilityConverter"/>
    </Application.Resources>
</Application>
